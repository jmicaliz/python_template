  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "example python cli launch",
      "type": "python",
      "request": "launch",
      "program": "/home/user/.pyenv/versions/3.8.3/envs/example_venv/bin/example-cli",
      "console": "integratedTerminal",
      "args": ["example_argument"],
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
